        "github.com/devopsfaith/krakend-jose/validator": {
          "alg": "HS256"
        }
      },
      "concurrent_calls": 1,
      "backend": [
        {
          "url_pattern": "/fdsaas/exit",
          "encoding": "no-op",
          "sd": "static",
          "method": "POST",
          "extra_config": {},
          "host": [
            "https://192.168.1.241",
            "http://localhost"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
